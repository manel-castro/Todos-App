{
  "name": "TodosApp-webpack",
  "version": "1.0.0",
  "engines": {
    "node": "12.18.4",
    "yarn": "1.22.10"
  },
  "description": "",
  "private": true,
  "scripts": {
    "start": "run-p start:api start:dev",
    "start:dev": "webpack serve --config webpack.config.dev.js --port 3000",
    "prestart:api": "node tools/createMockDb.js",
    "start:api": "node tools/apiServer.js",
    "build": "webpack --config webpack.config.prod.js",
    "clean:build": "rimraf ./build && mkdir build",
    "prebuild": "run-p clean:build",
    "postbutild": "run-p serve:build",
    "serve:build": "http-server ./build",
    "stage": "webpack --config webpack.config.stage.js",
    "clean:stage": "rimraf ./stage && mkdir stage",
    "prestage": "run-p clean:stage",
    "poststage": "run-p map:stage serve:stage",
    "serve:stage": "http-server ./stage",
    "map:stage": "source-map-explorer ./stage/bundle.js",
    "heroku-postbuild": "webpack --config webpack.config.prod.js --progress"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "babel-plugin-styled-components": "^1.11.1",
    "bootstrap": "^4.5.2",
    "firebase": "^7.20.0",
    "firebase-tools": "^8.10.0",
    "firebaseui": "^4.6.1",
    "history": "^5.0.0",
    "immer": "7.0.14",
    "prettier": "^2.1.2",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "^3.11.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.1.1",
    "react-toastify": "6.0.9",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "source-map-explorer": "^2.5.0",
    "styled-components": "^5.2.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-preset-react-app": "10.0.0",
    "css-loader": "5.0.0",
    "cssnano": "4.1.10",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "eslint": "7.12.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "7.21.5",
    "fetch-mock": "9.10.7",
    "html-webpack-plugin": "4.5.0",
    "http-server": "0.12.3",
    "jest": "26.6.1",
    "json-server": "0.16.2",
    "mini-css-extract-plugin": "1.2.0",
    "node-fetch": "^2.3.0",
    "npm-run-all": "4.1.5",
    "postcss-loader": "4.0.4",
    "react-test-renderer": "17.0.1",
    "react-testing-library": "8.0.1",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.4",
    "rimraf": "3.0.2",
    "style-loader": "2.0.0",
    "webpack": "5.2.0",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-cli": "4.1.0",
    "webpack-dev-server": "3.11.0",
    "webpack-monitor": "^1.0.14"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ],
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "pure": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "import/no-named-as-default": "off",
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "root": true
  }
}
